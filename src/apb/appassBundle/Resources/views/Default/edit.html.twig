    <script>
        $(function(){
            $(function(){
                $('#apb_appassbundle_mappearance_iconNameFile').fileUpload({
                    showImage: true,
                    {% if iconImage == '' %}
                        imgSrc : '{{ asset('bundles/apbappass/Images/mCouponsLogoIcon58x58.png') }}',
                    {% else %}
                        imgSrc : '{{ asset('upload/' ~ task2.id ~ '/' ~ iconImage) }}',
                    {% endif %}
                    hiddenField: '#apb_appassbundle_mappearance_iconName'
                });
                $('#apb_appassbundle_mappearance_logoNameFile').fileUpload({
                    {% if logoName != '' %}
                        imageCaption: '{{ logoName }}',
                    {% endif %}
                    hiddenField: '#apb_appassbundle_mappearance_logoName'
                });

                $('#apb_appassbundle_mappearance_couponStripFile').fileUpload({
                    {% if couponStrip != '' %}
                        imageCaption: '{{ couponStrip }}',
