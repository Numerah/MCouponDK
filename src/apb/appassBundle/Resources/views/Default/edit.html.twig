                    hiddenField: '#apb_appassbundle_mappearance_couponStrip'
                });
                $('#apb_appassbundle_mappearance_genericThumbnailFile').fileUpload({
                    {% if genericThumbnail != '' %}
                        imageCaption: '{{ genericThumbnail }}',
                    {% endif %}
                    hiddenField: '#apb_appassbundle_mappearance_genericThumbnail'
                });
                $('#apb_appassbundle_mappearance_boardingPassFooterFile').fileUpload({
                    {% if boardingPassFooter != '' %}
                    imageCaption: '{{ boardingPassFooter }}',
                    {% endif %}
                    hiddenField: '#apb_appassbundle_mappearance_boardingPassFooter'
